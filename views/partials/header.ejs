    <header>
        <h2>Message Board</h2>
        <% if (locals.isUserLogin && isUserLogin) { %>
            <p>Connected As <%= user.username %></p>
            <li><a href="/log-out">Log out</a></li>
        <% } else { %>
        <ul>
            <li><a href="/sign-up">Sign Up</a></li>
            <li><a href="/log-in">Log In</a></li>
        </ul>
        <% } %>
    </header>